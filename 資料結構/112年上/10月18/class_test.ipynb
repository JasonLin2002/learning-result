    "class Node:\n",
    "    def __init__(self, data:int):\n",
    "        self.data = data\n",
    "        self.next = None\n",
    "\n",
    "class Linked_List:\n",
    "    def __init__(self):\n",
    "        self.head = None\n",
    "    \n",
    "    def append(self, data:int):\n",
    "        if (not self.head):\n",
    "            self.head = Node(data)\n",
    "        else:\n",
    "            temp = self.head\n",
    "            while (temp.next):\n",
    "                temp = temp.next\n",
    "            temp.next = Node(data)\n",
    "    \n",
    "    def traverse(self):\n",
    "        temp = self.head\n",
    "        while (temp):\n",
    "            print(temp.data, \"-> \", end=\"\")\n",
    "            temp = temp.next\n",
    "        print(\"NULL\")\n",
    "      \n",
    "    def search(self, target:int)->int:\n",
    "        index = -1\n",
    "        temp = self.head\n",
    "        while (temp):\n",
    "            if (temp.data == target):\n",
    "                return index+1\n",
    "            temp = temp.next\n",
    "            index += 1\n",
    "        return -1\n",
    "\n",
    "    def insert(self, index:int, data:int):\n",
    "        if (index >= 0):\n",
    "            temp = self.head\n",
    "            self.head = Node(100)\n",
    "            self.head.next = temp\n",
    "            temp = self.head\n",
    "            pre = self.head.next\n",
    "            curr_index = 0\n",
    "            while (pre):\n",
    "                if (curr_index == index):\n",
    "                    temp.next = Node(data)\n",
    "                    temp.next.next = pre\n",
    "                    self.head = self.head.next\n",
    "                    return\n",
    "                curr_index += 1\n",
    "                pre = pre.next\n",
    "                temp = temp.next\n",
    "            temp.next = Node(data)\n",
    "            self.head = self.head.next\n",
    "        else:\n",
    "            print(\"Error index\")\n",
    "        \n",
    "    def delete(self, index:int):\n",
    "        if (self.head):\n",
    "            if (index >= 0):\n",
    "                temp = self.head\n",
    "                self.head = Node(100)\n",
    "                self.head.next = temp\n",
    "                temp = self.head\n",
    "                curr_index = -1\n",
    "                while (temp.next):\n",
    "                    if (curr_index+1 == index):\n",
    "                        del_node = temp.next\n",
    "                        temp.next = temp.next.next\n",
    "                        del del_node\n",
    "                        self.head = self.head.next\n",
    "                        return\n",
    "                    curr_index += 1\n",
    "                    temp = temp.next\n",
    "                print(\"The index out of the range\")\n",
    "                self.head = self.head.next\n",
    "            else:\n",
    "                print(\"The list is empty\")\n",
    "\n",
    "    def update(self, index:int, data:int):\n",
    "        if (index >= 0):\n",
    "            temp = self.head\n",
    "            curr_index = 0\n",
    "            while (temp):\n",
    "                if (curr_index == index):\n",
    "                    temp.data = data\n",
    "                    return\n",
    "                curr_index += 1\n",
    "                temp = temp.next\n",
    "            print(\"The index out of the range\")\n",
    "        else:\n",
    "            print(\"Error index\")\n",
    "  \n",
    "    def count(self, target:int)->int:\n",
    "        temp = self.head\n",
    "        count = 0\n",
    "        while (temp):\n",
    "            if (temp.data == target):\n",
    "                count += 1\n",
    "            temp = temp.next\n",
    "        return count\n",
    "    \n",
    "    def addSort(self, data:int):\n",
    "        temp = self.head\n",
    "        self.head = Node(0)\n",
    "        self.head.next = temp\n",
    "        temp = self.head\n",
    "        pre = temp.next\n",
    "        while (pre and data > pre.data):\n",
    "            temp = temp.next\n",
    "            pre = pre.next\n",
    "        temp.next = Node(data)\n",
    "        temp.next.next = pre\n",
    "        self.head = self.head.next\n",
    "\n",
