    "class node:\n",
    "    def __init__(self,data=None):\n",
    "        self.data=data\n",
    "        self.next=None #節點可以自己創幾格，這裡第一格有資料，第二格沒有\n",
    "        \n",
    "class Linkedlist:\n",
    "    def __init__(self):\n",
    "        self.head=None\n",
    "        \n",
    "    def add (self,data):     \n",
    "        if self.head==None: \n",
    "           self.head=node(data) #添加資料\n",
    "        else:\n",
    "            currentNode = self.head\n",
    "            while currentNode.next != None:\n",
    "                currentNode = currentNode.next\n",
    "                \n",
    "            currentNode.next= node(data)\n",
    "\n",
    "    def traverse(self):  #印出資料\n",
    "        if self.head==None:\n",
    "            print(\"NO Data\")\n",
    "        else:\n",
    "            currentNode = self.head\n",
    "            while currentNode.next != None:\n",
    "                print(currentNode.data,\"->\",end='')\n",
    "                currentNode=currentNode.next\n",
    "            print(currentNode.data,\"-->END\")\n",
    "            \n",
    "    def Search(self , data):\n",
    "        index = 1\n",
    "        flag = 0\n",
    "        currentNode = self.head\n",
    "        while currentNode.next != None  and  currentNode.data != data:\n",
    "            index += 1\n",
    "            currentNode = currentNode.next\n",
    "            if currentNode.data==data:\n",
    "                flag = 1\n",
    "                \n",
    "        if currentNode.data==data:\n",
    "            flag = 1\n",
    "        return index if flag else -1\n",
    "    \n",
    "    def insert(self,index,nwdata):\n",
    "        currentNode = None\n",
    "        prevNode = None\n",
    "        if self.head==None and index ==1: \n",
    "           self.head=node(nwdata)\n",
    "        elif self.head == None and index>1:\n",
    "            print('Error')   \n",
    "        else:\n",
    "            currentNode = self.head\n",
    "            prevNode =self.head\n",
    "            i=1\n",
    "            while i != index and currentNode.next != None:\n",
    "                i+=1\n",
    "                prevNode= currentNode\n",
    "                currentNode = currentNode.next\n",
    "            if i==index:\n",
    "                prevNode.next= node(nwdata)\n",
    "                prevNode.next.next = currentNode\n",
    "\n",
    "    def delete(self, index):\n",
    "        if index < 1:\n",
    "            print(\"Invalid index\")\n",
    "            return\n",
    "        elif index == 1:\n",
    "            if self.head is not None:\n",
    "                self.head = self.head.next\n",
    "            else:\n",
    "                print(\"List is empty\")\n",
    "            return\n",
    "        else:\n",
    "            i = 1\n",
    "            currentNode = self.head\n",
    "            prevNode = None\n",
    "            while i < index and currentNode is not None:\n",
    "                prevNode = currentNode\n",
    "                currentNode = currentNode.next\n",
    "                i += 1\n",
    "            if currentNode is not None:\n",
    "                prevNode.next = currentNode.next\n",
    "            else:\n",
    "                print(\"Index out of range\")\n",
    "\n",
    "    def update(self, index, new_data):\n",
    "        if index < 1:\n",
    "            print(\"Invalid index\")\n",
    "            return\n",
    "        else:\n",
    "            i = 1\n",
    "            currentNode = self.head\n",
    "            while i < index and currentNode is not None:\n",
    "                currentNode = currentNode.next\n",
    "                i += 1\n",
    "            if currentNode is not None:\n",
    "                currentNode.data = new_data\n",
    "            else:\n",
    "                print(\"Index out of range\")\n",
    "\n",
    "\n",
