   "source": [
    "import torch\n",
    "x = torch.tensor( 3. )\n",
    "x.requires_grad_()\n",
    "y = (10*x**2)/(5*x**2+6)\n",
    "y.backward ()\n",
    "x.grad"
   ]
  },
