   "source": [
    "import tensorflow as tf\n",
    "x= tf.Variable( 3.0 )\n",
    "with tf.GradientTape() as t:\n",
    "    t.watch(x)\n",
    "    y = (10*x**2)/(5*x**2+6)\n",
    "t.gradient(y,x)"
   ]
  }
